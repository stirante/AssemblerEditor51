<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0"
      prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
   <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
      <opaqueInsets>
         <Insets/>
      </opaqueInsets>
      <VBox.margin>
         <Insets/>
      </VBox.margin>
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
      </padding>
      <Label alignment="CENTER_RIGHT" prefWidth="50.0" text="Find: "/>
      <TextField fx:id="findField" HBox.hgrow="ALWAYS"/>
   </HBox>
   <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
      <opaqueInsets>
         <Insets/>
      </opaqueInsets>
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
      </padding>
      <Label alignment="CENTER_RIGHT" prefWidth="50.0" text="Replace: "/>
      <TextField fx:id="replaceField" HBox.hgrow="ALWAYS"/>
   </HBox>
   <HBox alignment="CENTER_LEFT">
      <padding>
         <Insets left="10.0" right="10.0"/>
      </padding>
      <Label fx:id="status" alignment="CENTER" textAlignment="CENTER" HBox.hgrow="ALWAYS"/>
   </HBox>
</VBox>
